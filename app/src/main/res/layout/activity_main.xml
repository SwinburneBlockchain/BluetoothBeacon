<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.swinblockchain.bluetoothbeacon.UI.MainActivity">

    <TextView
        android:id="@+id/consoleWindow"
        android:layout_width="370dp"
        android:layout_height="456dp"
        android:layout_marginTop="7dp"
        android:text="Console\n"
        android:textAlignment="viewStart"
        android:textSize="12sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/producerName"
        android:layout_width="222dp"
        android:layout_height="29dp"
        android:text="Producer:"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.695"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/adminButton"
        android:layout_marginBottom="7dp" />

    <Button
        android:id="@+id/adminButton"
        android:layout_width="123dp"
        android:layout_height="48dp"
        android:onClick="startAdmin"
        android:text="ADMIN"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="-1dp"
        android:layout_marginRight="16dp" />

</android.support.constraint.ConstraintLayout>
